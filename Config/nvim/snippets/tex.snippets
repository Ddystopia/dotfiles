    $0
  \end{document}

snippet fancy "Fancy" b
  \pagestyle{fancy}
  \fancyhf{}
  \fancyhfoffset[L]{5mm} % left extra length
  \fancyhfoffset[R]{5mm} % right extra length
  \lhead{\bfseries Oleksandr Babak, \newline ID: 122965}
  \rhead{\today}
  \fancyfoot[R]{\thepage}
  \thispagestyle{fancy}


