  \usepackage[utf8]{inputenc}
  \usepackage[T1]{fontenc}
  \usepackage{textcomp}
  \usepackage[slovak]{babel}
  \usepackage{amsmath, amssymb}
  \usepackage[margin={1.7cm, 2.5cm}]{geometry}
  \usepackage{fancyhdr}


  % figure support
  % \usepackage{import}
  % \usepackage{xifthen}
  \pdfminorversion=7
  \usepackage{pdfpages}
  % \usepackage{transparent}
  % \newcommand{\incfig}[1] {%
  %  \def\svgwidth{\columnwidth}
  %  \import{./figures/}{#1.pdf_tex}
  % }

