{
  "#!": {
    "prefix": "#!",
    "body": [
      "#!/usr/bin/env lua",
      "$1"
    ]
  },
  "local": {
    "prefix": "local",
    "body": "local ${1:x} = ${0:1}"
  },
  "fun": {
    "prefix": "fun",
    "body": [
      "function ${1:fname}(${2:...})",
      "\t$0",
      "end"
    ]
  },
  "for": {
    "prefix": "for",
    "body": [
      "for ${1:i}=${2:1},${3:10} do",
      "\t${0:print(i)}",
      "end"
    ]
  },
  "forp": {
    "prefix": "forp",
    "body": [
      "for ${1:i},${2:v} in pairs(${3:table_name}) do",
      "\t$0",
      "end"
    ]
  },
  "fori": {
    "prefix": "fori",
    "body": [
      "for ${1:i},${2:v} in ipairs(${3:table_name}) do",
      "\t$0",
      "end"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "if $1 then",
      "\t$2",
      "end"
    ]
  },
  "ife": {
    "prefix": "ife",
    "body": [
      "if $1 then",
      "\t${2:-- if condition}",
      "else",
      "\t${0:-- else}",
      "end"
    ]
  },
  "elif": {
    "prefix": "elif",
    "body": [
      "elseif $1 then",
      "\t$0"
    ]
  },
  "repeat": {
    "prefix": "repeat",
    "body": [
      "repeat",
      "\t$1",
      "until $0"
    ]
  },
  "while": {
    "prefix": "while",
    "body": [
      "while $1 do",
      "\t$0",
      "end"
    ]
  },
  "wh": {
    "prefix": "wh",
    "body": [
      "while ${1:true} do",
      "\t$0",
      "end"
    ]
  },
  "wht": {
    "prefix": "wht",
    "body": [
      "while true do",
      "\t$0",
      "end"
    ]
  },
  "print": {
    "prefix": "print",
    "body": "print(\"${1:string}\")"
  },
  "pr": {
    "prefix": "pr",
    "body": "print($0)"
  },
  "prs": {
    "prefix": "prs",
    "body": "print(\"$0\")"
  },
  "prf": {
    "prefix": "prf",
    "body": "print(string.format(\"${1:%s}\"$0))"
  },
  "wr": {
    "prefix": "wr",
    "body": "io.write($0)"
  },
  "wrs": {
    "prefix": "wrs",
    "body": "io.write(\"$0\")"
  },
  "wrf": {
    "prefix": "wrf",
    "body": "io.write(string.format(\"${1:%s}\"$0))"
  },
  "fwr": {
    "prefix": "fwr",
    "body": "io.${1:stderr}:write($0)"
  },
  "fwrs": {
    "prefix": "fwrs",
    "body": "io.${1:stderr}:write(\"$0\")"
  },
  "fwrf": {
    "prefix": "fwrf",
    "body": "io.${1:stderr}:write(string.format(\"${2:%s}\"$0))"
  },
  "im": {
    "prefix": "im",
    "body": "import \"${1:import file}\""
  }
}
