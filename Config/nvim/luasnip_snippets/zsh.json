{
  "#!": {
    "prefix": "#!",
    "body": [
      "#!/usr/bin/env zsh",
      ""
    ]
  },
  "case": {
    "prefix": "case",
    "body": [
      "case ${1:word} in",
      "\t${2:pattern})",
      "\t\t$0;;",
      "esac"
    ]
  },
  "(": {
    "prefix": "(",
    "body": "( ${0:#statements} )"
  },
  "{": {
    "prefix": "{",
    "body": "{ ${0:#statements} \\}"
  },
  "[": {
    "prefix": "[",
    "body": "[[ ${0:test} ]]"
  },
  "always": {
    "prefix": "always",
    "body": "{ ${1:try} \\} always { ${0:always} \\}"
  },
  "fun": {
    "prefix": "fun",
    "body": [
      "${1:function_name}() {",
      "\t${0:# function_body}",
      "\\}"
    ]
  },
  "ffun": {
    "prefix": "ffun",
    "body": [
      "function ${1:function_name}() {",
      "\t${0:# function_body}",
      "\\}"
    ]
  }
}
